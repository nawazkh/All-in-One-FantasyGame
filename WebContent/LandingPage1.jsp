<script>
var flag=1
setInterval(function() {
	var obj = document.getElementById("imageArrow");
    var val = parseFloat(window.getComputedStyle(obj).getPropertyValue("opacity"));
    if(val == 0) {
    	flag=0;
    	obj.style.opacity = (val + 0.1);
    } else if (val == 1) {
    	flag=1;
    	obj.style.opacity = (val - 0.1);
    } else {
	    if(flag==1){
	    	obj.style.opacity = (val - 0.1);
	    } else {
	    	obj.style.opacity = (val + 0.1);
	    }
    }
}, 125);

